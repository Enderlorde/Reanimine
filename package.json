{
  "name": "reanicraft",
  "version": "0.0.1",
  "description": "Simple minecraft launcher for my server",
  "main": "electron_start.js",
  "scripts": {
    "start": "concurrently \"webpack serve\" \"yarn electron:dev\" ",
    "dist": "webpack --mode=production --node-env=production & electron-builder",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "electron:dev": "set ELECTRON_START_URL=http://localhost:3000 & wait-on tcp:3000 & electron ."
  },
  "build": {
    "appId": "com.launcher.reanicraft",
    "asar": true,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "electron_start.js",
      "preload.js"
    ],
    "fileAssociations": {
      "ext": "png",
      "icon": "./icon.png"
    }
  },
  "author": "enderlorde",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^8.0.1",
    "@webpack-cli/generators": "^3.0.4",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.3",
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "html-webpack-plugin": "^5.5.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "@minecraft-js/nbt": "^1.1.0",
    "@wiicamp/react-minecraft-skin-viewer": "^1.0.6",
    "@xmcl/nbt": "^3.0.1",
    "ajv": "^8.12.0",
    "lodash": "^4.17.21",
    "minecraft-launcher-core": "^3.16.18",
    "node-downloader-helper": "^2.1.7",
    "open": "^8.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "skinview3d": "^3.0.0-alpha.1"
  }
}
