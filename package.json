    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^8.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-vite": "^1.0.22",
    "eslint": "^8.41.0",
